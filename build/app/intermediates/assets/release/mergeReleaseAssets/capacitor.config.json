{
	"appId": "com.catchy.fabricmarket",
	"appName": "Catchy Fabric Market",
	"webDir": "www",
	"bundledWebRuntime": false,
	"server": {
		"androidScheme": "https"
	},
	"plugins": {
		"SplashScreen": {
			"launchShowDuration": 2000,
			"backgroundColor": "#1A1F2C",
			"androidSplashResourceName": "splash",
			"androidSplashImageUrl": ""
		},
		"StatusBar": {
			"style": "DARK",
			"backgroundColor": "#1A1F2C",
			"translucent": false,
			"overlaysWebView": false
		}
	}
}
